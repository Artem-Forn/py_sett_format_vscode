{
    "black-formatter.args": [
        "--line-length=119 ",
        "--target-version=['py38']",
        "--force-exclude=['__init__']"
      ],
    
      "isort.args": [
        "--profile=black",
        "--multi_line_output=7",
        "--py_version=38",
        "--single_line_exclusions=['odoo', 'odoo.exceptions', 'odoo.tools', 'odoo.addons']",
        "--force-single-line-imports"
      ] 
}
